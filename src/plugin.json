{
  "name": "AIOCR",
  "version": "1.0.0",
  "pluginName": "AIOCR",
  "description": "基于多模态大模型的OCR识别工具",
  "main": "index.html",
  "logo": "logo.png",
  "preload": "preload.js",
  "features": [
    {
      "code": "ai_ocr",
      "explain": "AI OCR 识别",
      "cmds": [
        "翻译",
        "ocr",
        "识别",
        "截图文字识别",
        "文字识别",
        {
          "type": "img",
          "label": "OCR 识别图片"
        },
        "图片文字识别",
        {
          "type": "files",
          "label": "提取图片文字",
          "fileType": "file",
          "match": "/\\.(jpg|jpeg|png|gif|bmp)$/i",
          "minLength": 1,
          "maxLength": 1
        }
      ]
    }
  ]
}